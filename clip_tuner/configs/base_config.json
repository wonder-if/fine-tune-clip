{
    "model": {
        "name": "resnet50",
        "pretrained": true,
        "num_labels": 20
    },
    "data": {
        "image_size": 224,
        "image_mean": [
            0.485,
            0.456,
            0.406
        ],
        "image_std": [
            0.229,
            0.224,
            0.225
        ],
        "blur": false
    },
    "training": {
        "batch_size": 64,
        "optimizer": "sgd",
        "lr": 0.001,
        "lr_scheduler": "cosine",
        "num_epochs": 100
    }
}